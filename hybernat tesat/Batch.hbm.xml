<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
      <hibernate-mapping>
          <class name="Batch" table="BATCH">
               <meta attribute="class-description">
               </meta>
               <id name="batchId" type="int" column="batch_id">
                     <generator class="native"/>
               </id>
               <property name="progId" column="prog_id" type="int"/>
               <property name="batchYear" column="batch_year" type="int"/>
               <property name="shift" column="shift" type="string"/>
               <property name="groupDesc" column="group_desc" type="string"/>
               <property name="remarks" column="remarks" type="string"/>
               <property name="session" column="session" type="string"/>

           </class>
      </hibernate-mapping>
